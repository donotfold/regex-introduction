  "description": "Taskmanager for Angular projects",
  "README": "./README.md",
  "private": true,
  "scripts": {
    "prestart": "npm i",
    "start": "node ./node_modules/gulp/bin/gulp.js",
    "dev": "node ./node_modules/gulp/bin/gulp.js",
    "staging": "node ./node_modules/gulp/bin/gulp.js --env production",
    "poststaging": "cd .. && ng build --dev -op dist/staging",
    "production": "node ./node_modules/gulp/bin/gulp.js --env production",
    "postproduction": "cd .. && ng build --prod -op dist/production"
  },
  "devDependencies": {
